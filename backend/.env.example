# 应用配置
NODE_ENV=development
PORT=8000

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ai_evaluation
DB_USER=postgres
DB_PASSWORD=postgres123
DB_SSL=false

# JWT配置
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# CORS配置
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Redis配置（可选）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# 职教云API配置
ZJIJAOYUN_BASE_URL=https://api.zhijiaoyun.com
ZJIJAOYUN_API_KEY=your_api_key
ZJIJAOYUN_API_SECRET=your_api_secret

# 日志配置
LOG_LEVEL=info
LOG_FILE_ENABLED=false
LOG_FILE_PATH=./logs/app.log

# 安全配置
BCRYPT_ROUNDS=12

# 上传配置
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads