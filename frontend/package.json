{
  "name": "ai-evaluation-frontend",
  "version": "1.0.0",
  "description": "AI助力评价系统前端应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:staging": "NODE_ENV=staging VITE_APP_ENV=staging tsc && vite build",
    "build:production": "NODE_ENV=production VITE_APP_ENV=production tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:report": "eslint . --ext ts,tsx --format=json --output-file=eslint-report.json",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "build:profile": "vite build --mode profile",
    "start:prod": "serve -s dist -l 3000",
    "bundle-size": "bundlesize",
    "security:audit": "npm audit --audit-level=high",
    "pre-deploy": "npm run type-check && npm run lint && npm run test:coverage && npm run build",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "docker:build": "docker build -t ai-evaluation-frontend .",
    "docker:run": "docker run -p 3000:8080 ai-evaluation-frontend",
    "quality:check": "npm run type-check && npm run lint:report && npm run security:audit && npm run bundle-size"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-router": "^1.0.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.290.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^7.9.6",
    "react-select": "^5.8.0",
    "react-table": "^7.8.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash-es": "^4.17.11",
    "@types/react": "^18.2.37",
    "@types/react-beautiful-dnd": "^13.1.6",
    "@types/react-dom": "^18.2.15",
    "@types/react-table": "^7.7.17",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.34.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "ai",
    "evaluation",
    "education",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "author": "HKHR Project Team",
  "license": "MIT"
}
